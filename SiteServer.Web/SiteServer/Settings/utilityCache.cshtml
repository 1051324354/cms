@{ Layout = "../Shared/_Layout.cshtml"; }

<el-card>
  <div slot="header" class="clearfix">
    <span>系统缓存</span>
  </div>

  <p class="text-muted font-13">
    当前缓存数：{{ count }}
    <el-button type="primary" style="margin-left: 10px;" size="small" v-on:click="btnCleanClick">清除缓存</el-button>
  </p>

  <el-table
    :data="parameters"
    style="width: 100%">
    <el-table-column
      prop="key"
      label="缓存键"
      width="280">
    </el-table-column>
    <el-table-column
      prop="value"
      label="缓存值">
    </el-table-column>
  </el-table>

</el-card>



@section Scripts{ <script src="utilityCache.js" type="text/javascript"></script> }